    "cover": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "date": {
      "type": "datetime"
    },
    "blog_category": {
      "model": "blog-categories"
    }
  }
}
